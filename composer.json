{
    "name": "kresnasatya/wp-vite-theme",
    "type": "project",
    "description": "Generic WordPress theme integrate with ViteJS",
    "keywords": ["WordPress", "ViteJS", "theme"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy ('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
